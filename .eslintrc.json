{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "extends": "airbnb-base",
    "rules": {
        "class-methods-use-this": 0,
        "indent": [2, 4],
        "import/prefer-default-export": 0,
        "max-len": 0,
        "no-param-reassign": ["error", { "props": false }],
        "no-underscore-dangle": 0,
        "no-use-before-define": [2, "nofunc"]
    }
}
